// standard PC keymap with the following additions:
// - types: "+command_option"
// - symbols: "+compose(rctrl)+macos+tenkeyless"
xkb_keymap {
	xkb_keycodes  { include "evdev+aliases(qwerty)"	};
	xkb_types     { include "complete+command_option"	};
	xkb_compat    { include "complete"	};
	xkb_symbols   { include "pc+us+inet(evdev)+compose(rctrl)+macos(swap_alt_win_no_meta)+tenkeyless"	};
	xkb_geometry  { include "pc(pc105)"	};
};
