<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>KeepAlive</key>
	<true/>
	<key>Label</key>
	<string>com.linacreative.Synergy</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/bin/synergy-start.sh</string>
	</array>
	<key>StandardErrorPath</key>
	<string>/tmp/synergy.stderr</string>
	<key>StandardOutPath</key>
	<string>/tmp/synergy.stdout</string>
</dict>
</plist>
