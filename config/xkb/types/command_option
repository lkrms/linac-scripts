partial default xkb_types "command_option" {

    type "COMMAND_OPTION" {
        modifiers= Control+Mod4+Alt;
        map[Mod4]= Level2;
        map[Alt]= Level3;
        map[Control+Alt]= Level4;
        level_name[Level1]= "Base";
        level_name[Level2]= "Command";
        level_name[Level3]= "Option";
        level_name[Level4]= "Control Option";
    };

};
